                    <fluent:CheckBox Header="Klausuren" IsChecked="{Binding SyncExams, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Klausren importieren" Text="Wenn diese Option ausgewählt ist, werden Klausuren importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Räume" IsChecked="{Binding SyncRooms, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Räume importieren" Text="Wenn diese Option ausgewählt ist, werden Räume importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Vertretungen" IsChecked="{Binding SyncSubstitutions, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Vertretungen importieren" Text="Wenn diese Option ausgewählt ist, werden Vertretungen importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Aufsichten" IsChecked="{Binding SyncSupervisions, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Aufsichten importieren" Text="Wenn diese Option ausgewählt ist, werden Aufsichten importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Stundenplan" IsChecked="{Binding SyncTimetable, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Stundenplan importieren" Text="Wenn diese Option ausgewählt ist, wird der Stundenplan importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Unterrichte" IsChecked="{Binding SyncTuitions, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Unterrichte importieren" Text="Wenn diese Option ausgewählt ist, werden Unterrichte importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                </fluent:RibbonGroupBox>

