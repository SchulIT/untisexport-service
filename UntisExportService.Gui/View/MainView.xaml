        <ListView x:Name="loggerListView">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Level" DisplayMemberBinding="{Binding Level}" Width="100" />
                    <GridViewColumn Header="Datum" DisplayMemberBinding="{Binding TimeStamp, StringFormat=g, ConverterCulture=de}" Width="100" />
                    <GridViewColumn Header="Service" DisplayMemberBinding="{Binding LoggerName}" Width="200" />
                    <GridViewColumn Header="Mitteilung" DisplayMemberBinding="{Binding Message}" Width="800" />
                </GridView>
            </ListView.View>
        </ListView>
    </DockPanel>
